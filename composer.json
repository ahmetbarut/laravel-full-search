{
    "name": "ahmetbarut/laravel-full-search",
    "description": "Allows searching on multiple models. The advantage of this for us; a separate search field is not required for each model.",
    "type": "package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AhmetBarut\\FullSearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ahmet Barut",
            "email": "ahmetbarut588@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmetBarut\\FullSearch\\Providers\\FullSearchServiceProvider"
            ]
        }
    }
}
